@{
    ViewData["Title"] = "Home Page";
}
@model PaginatedList<HouseVin.Data.t_houseInfo>

<div class="py-5">
    <div class="container">

        <div class="row">
            <div class="col-md-6 col-lg-4 mb-3 mb-lg-0">
                <div class="feature d-flex align-items-start">
                    <span class="icon mr-3 flaticon-house"></span>
                    <div class="text">
                        <h2 class="mt-0">Nhà đẹp</h2>
                        <p></p>
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-lg-4 mb-3 mb-lg-0">
                <div class="feature d-flex align-items-start">
                    <span class="icon mr-3 flaticon-rent"></span>
                    <div class="text">
                        <h2 class="mt-0">Thuê hoặc bán</h2>
                        <p></p>
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-lg-4 mb-3 mb-lg-0">
                <div class="feature d-flex align-items-start">
                    <span class="icon mr-3 flaticon-location"></span>
                    <div class="text">
                        <h2 class="mt-0">Vị trí tốt</h2>
                        <p></p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Search box DOWN -->
<div class="site-section py-5">
    <div class="container">

        <div class="col-md-6 col-lg-4 mb-4">
            <h3>Tìm kiếm</h3>
            <form asp-action="Search">
                <div class="row">
                    <div class="col-md-6 col-lg-3 mb-3 mb-lg-0">
                        <input class="form-control" name="searchMaNha" placeholder="Nhập tên nhà để tìm " />
                    </div>

                </div>
            </form>
        </div>

        <div class="col-md-6 col-lg-4 mb-4">
            <h3>Loc gia cao thap</h3>
            <form asp-action="Search">
                <div class="row">
                    <div class="col-md-6 col-lg-3 mb-3 mb-lg-0">
                        <input class="form-control" name="searchMaNha" placeholder="Nhập tên nhà để tìm " />
                    </div>

                </div>
            </form>
        </div>

    </div>
</div>

<!-- Search box UP   -->


<div class="site-section site-section-sm bg-light">
    <div class="container">
        <div class="row mb-5">
            @foreach (var item in Model)
            {
                <div class="col-md-6 col-lg-4 mb-4">
                    <a asp-route-id="@item.Id" asp-action="Detail" class="prop-entry d-block">
                        <figure>
                            <img src="~/images/@item.Thumbnail" alt="Image" class="img-fluid">
                        </figure>
                        <div class="prop-text">
                            <div class="inner">
                                <span class="price rounded">@Html.DisplayFor(modelItem => item.HousePrice)</span>
                                <h3 class="title"> @Html.DisplayFor(modelItem => item.HouseName)</h3>
                                <p class="location">@Html.DisplayFor(modelItem => item.HouseAreas)</p>
                            </div>
                            <div class="prop-more-info">
                                <div class="inner d-flex">
                                    <div class="col">
                                        <span>Acreage:</span>
                                        <strong>@Html.DisplayFor(modelItem => item.HouseAcreage)m<sup>2</sup></strong>
                                    </div>
                                    <div class="col">
                                        <span>Areas:</span>
                                        <strong>@Html.DisplayFor(modelItem => item.HouseAreas)</strong>
                                    </div>
                                    <div class="col">
                                        <span>Baths:</span>
                                        <strong>2</strong>
                                    </div>
                                    <div class="col">
                                        <span>Garages:</span>
                                        <strong>1</strong>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </a>
                </div>
            }
        </div>
        @{
            var prevDisabled = !Model.HasPreviousPage ? "disabled" : "";
            var nextDisabled = !Model.HasNextPage ? "disabled" : "";
        }
        <div class="row">
            <div class="col-md-12 text-center">
                <div class="site-pagination">
                    @{
                        for (var i = 1; i <= Model.TotalPages; i++)
                        {
                            <a asp-action="Index" asp-route-pageNumber="@i" class=" @(i == Model.PageIndex ? "active" : "")">@i</a>
                        }
                    }

                </div>
            </div>
        </div>

    </div>
</div>

